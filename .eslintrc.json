{
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "no-undef": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
